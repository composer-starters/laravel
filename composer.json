{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "starter/laravel",
    "type": "project",
    "description": "A skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework", "skeleton", "skeletor", "starter", "composer-starter"],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "laravel/framework": "^11.31",
        "laravel/tinker": "^2.10"
    },
    "require-dev": {
        "aaronfrancis/solo": "^0.1.2",
        "aniftyco/skeletor": "dev-master",
        "fakerphp/faker": "^1.24",
        "laravel/pint": "^1.18",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.5",
        "phpunit/phpunit": "^11.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-create-project-cmd": [
            "NiftyCo\\Skeletor\\Runner::execute"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "php artisan solo"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
